<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ユーザー登録 -->
<mapper namespace="com.example.entity.MUser">
	<insert id="insertOne">
		insert into m_user(
			 user_name
			,password
		)
		
		values(
		  #{userName}
		 ,#{password}
		)
	</insert>
</mapper>